@CHARSET "ISO-8859-1";

.btreemenu ul {
	font-family: "Trebuchet MS", Helvetica, Sans-Serif;
	font-size: 14px;
	list-style-type: none;
}

.btreemenu li {
	font-family: "Trebuchet MS", Helvetica, Sans-Serif;
	font-size: 14px;
	color: gray;
	text-decoration: none;
	padding-bottom: 0px;
}

.btreemenu a
{
	text-decoration:none;
	font-size: 13px;
	padding-left: 10px;
}

.btreemenu li,.btreemenu ul{
	padding-left: 0px;
}

.btreemenu li ul{
	padding-left: 2px;
}

.btreemenu li ul li{
	padding-left: 8px;
}

.btreemenu ul .open {
	display: block;
}

.btreemenu ul .close {
	display: none;
}

.btreemenu .title {
	margin:5px 0px 2px 0px;
	padding:5px 0px 5px 8px;
	background:Lavender;
	font-weight:bold;
}