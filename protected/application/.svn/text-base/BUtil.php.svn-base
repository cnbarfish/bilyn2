class BUtil {
    
    private $menuManager = null;
    
    /**
     *
     * @return BMenuManager 
     */
    public function getMenuManager()
    {
        if($this->menuManager == null)
            $this->menuManager = new BMenuManager ();
        
        return $this->menuManager;
    }
}

